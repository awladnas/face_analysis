<div class = "well">
  <%= form_for @profile, url: profile_index_path ,html: { multipart: true } do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
      <%= f.submit "Save", class: "btn btn-primary"  %>
  <% end %>
</div>